{
    "name": "Miox",
    "manifest_version": 2,
    "version": "0.1.7",
    "description": "Mioを使いやすくするよ",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["miox.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "options_page": "options.html",
    "permissions": ["tabs", "storage", "notifications", "<all_urls>"],
    "web_accessible_resources": ["loading.gif"]
}
